applications:
- services:
  - International Bot Machines Mystery
  host: bluedo2-stage
  disk: 512M
  name: bluedo2-stage
  command: node app.js
  path: .
  domain: mybluemix.net
  memory: 512M
  instances: 1
  env:
    CONVERSATION_SERVICE: <SERVICE_NAME>
    WORKSPACE_MANAGER: <WORKSPACE_ID>
    WORKSPACE_DESIGNER: <WORKSPACE_ID>
    WORKSPACE_DEVELOPER: <WORKSPACE_ID>
    WORKSPACE_WATSON: <WORKSPACE_ID>
